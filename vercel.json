{
  "version": 2,
  "builds": [
    {
      "src": "main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.ts"
    },
    {
      "src": "/api",
      "dest": "/main.ts"
    },
    {
      "src": "/user/(.*)",
      "dest": "/main.ts"
    },
    {
      "src": "/user",
      "dest": "/main.ts"
    }
  ]
}
